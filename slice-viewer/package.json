{
  "name": "slicesplorer",
  "version": "0.0.1",
  "description": "Viewer of slices",
  "main": "support/index.js",
  "keywords": [
    "pux",
    "purescript-pux",
    "boilerplate",
    "starter-app"
  ],
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "clean": "rimraf static/dist && rimraf dist && rimraf output",
    "build": "webpack --config ./webpack.production.config.js --progress --profile --colors",
    "watch": "node ./webpack.config.js",
    "serve": "http-server static --cors -p 3000",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gabysbrain/slicesplorer.git"
  },
  "author": "Thomas Torsney-Weir",
  "license": "Apache-2.0",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "d3": "^4.5.0",
    "purescript": "^0.10.1",
    "react": "^15.0.0",
    "react-addons-pure-render-mixin": "^15.0.1",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "bower": "*",
    "connect-history-api-fallback": "^1.2.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "node-sass": "^3.5.3",
    "purescript-psa": "^0.3.9",
    "purs-loader": "^2.0.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "through": "^2.3.8",
    "transform-loader": "^0.2.3",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-strip": "^0.1.0"
  }
}
